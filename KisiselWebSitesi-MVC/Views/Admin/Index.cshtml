@using KisiselWebSitesi_MVC.Models
@model List<AnaSayfa>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered mt-3">
    <tr>
        <th>ID</th>
        <th>İsim</th>
        <th>Güncelle</th>
    </tr>

    <tr>
        @foreach (var x in Model)
        {
            <td>@x.id</td>
            <td>@x.isim</td>
            <td> <a href="/Admin/AnaSayfaGetir/@x.id" class="btn btn-primary">Güncelle</a> </td>
        }

    </tr>
</table>
